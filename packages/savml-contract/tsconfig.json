{
  "compilerOptions": {
    "declaration": true,
    "target": "es6",
    "noImplicitAny": true,
    "sourceMap": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noImplicitThis": true,
    "diagnostics": true,
    "listFiles": true,
    "pretty": true,
    "moduleResolution": "Node",
    "noEmitOnError": false,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "strict": true,
    "outDir": "./dist",
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": []
}